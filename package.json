{
  "name": "project-root",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd shopping-cart && vite",
    "build": "cd shopping-cart && vite build",
    "lint": "eslint .",
    "preview": "cd shopping-cart && vite preview",
    "test": "concurrently \"npm run test:server\" \"npm run test:client\"",
    "test:server": "cd server && vitest run",
    "test:client": "cd shopping-cart && vitest run",
    "coverage": "concurrently \"npm run coverage:server\" \"npm run coverage:client\"",
    "coverage:server": "cd server && vitest run --coverage",
    "coverage:client": "cd shopping-cart && vitest run --coverage"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.10",
    "@react-oauth/google": "^0.12.1",
    "@reduxjs/toolkit": "^2.5.1",
    "@tailwindcss/vite": "^4.0.6",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "fs": "^0.0.1-security",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.5",
    "tailwindcss": "^4.0.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.19.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-istanbul": "^3.0.9",
    "concurrently": "^6.5.1",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "^26.0.0",
    "redux-mock-store": "^1.5.5",
    "supertest": "^7.0.0",
    "vite": "^6.1.0",
    "vite-node": "^3.0.8",
    "vitest": "^3.0.8"
  }
}
